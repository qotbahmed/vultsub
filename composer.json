    "yii2",
    "framework",
    "start",
    "cms",
    "application template",
    "yii2 advanced"
  ],
  "authors": [
    {
      "name": "Yevhen Terentiev",
      "email": "yevhen.terentiev@gmail.com"
    },
    {
      "name": "Victor Gonzalez",

    "kartik-v/yii2-password": "dev-master",
    "kartik-v/yii2-bootstrap4-dropdown": "@dev",
    "demi/sort": "~1.0",
    "twilio/sdk": "^6.36",
    "asofter/yii2-imperavi-redactor": "*",
    "kartik-v/yii2-widget-rating": "dev-master",
    "kartik-v/yii2-editors": "@dev",
    "himiklab/yii2-recaptcha-widget": "*",
    "kartik-v/yii2-datecontrol": "@dev",
    "aksafan/yii2-fcm-both-api": "^0.10.0",
    "curl/curl": "^2.5",
    "thamtech/yii2-uuid": "^1.2",
    "omnilight/yii2-scheduling": "*",
    "prokhonenkov/yii2-repeater": "^1.0",
    "yiisoft/yii2-bootstrap": "^2.0",
    "wbraganca/yii2-dynamicform": "^2.0",
    "unclead/yii2-multiple-input": "^2.30",
    "pigochu/yii2-jquery-locationpicker": "^0.2.5",
    "kartik-v/yii2-widget-colorinput": "*",
    "edofre/yii2-fullcalendar": "V1.0.11",
    "npm-asset/bs-stepper": "^1.7",
    "npm-asset/jquery-validation": "^1.21",
    "npm-asset/sweetalert2": "^11.11",
    "newerton/yii2-fancybox": "^1.0",
    "2amigos/yii2-chartjs-widget": "^3.0",
    "philippfrenzel/yii2fullcalendar":"*",
    "edofre/yii2-fullcalendar-scheduler": "^1.1",
    "npm-asset/momentjs": "^2.0",
    "endroid/qr-code": "^5.1",
    "miloschuman/yii2-highcharts-widget": "^11.0",
    "npm-asset/chart.js": "^4.4"
  },
  "require-dev": {
    "yiisoft/yii2-gii": "^2.0.0",
    "yiisoft/yii2-faker": "^2.0.0"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/",
      "prokhonenkov\\yii2repeater\\": "vendor/prokhonenkov/yii2-repeater/src/"

    }
  },
  "suggest": {
    "trntv/yii2-debug-xhprof": "dev-master@dev"
  },
  "config": {
    "process-timeout": 1800,
    "optimize-autoloader": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "archive": {
    "exclude": ["docs"]
  }

}
